    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/carbon": "^1.1.16",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@unocss/preset-attributify": "^0.52.0",
