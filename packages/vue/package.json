    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.6",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "terser": "^5.18.1",
    "typescript": "^4.9.5",
    "unocss": "^0.53.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.1"
