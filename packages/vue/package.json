{
  "name": "@starter/vue",
  "version": "0.4.3",
  "scripts": {
    "build": "vue-tsc && vite build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "dev": "vite",
    "lint": "eslint --fix ./src/**",
    "prepare": "husky install",
    "preview": "vite preview",
    "release": "bump --commit \"release: v%s\" --tag --push",
    "version": "pnpm changelog"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts, vue}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 0.5%"
  ],
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.39.8",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/carbon": "^1.1.18",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@unocss/preset-attributify": "^0.54.0",
    "@unocss/preset-icons": "^0.54.0",
    "@unocss/preset-wind": "^0.54.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.7",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "terser": "^5.19.2",
    "typescript": "^4.9.5",
    "unocss": "^0.54.0",
    "vite": "^4.4.7",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "pnpm@7.33.5"
}