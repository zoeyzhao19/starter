{
  "name": "@starter/vue",
  "version": "0.4.3",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint --fix ./src/**",
    "preview": "vite preview",
    "prepare": "husky install",
