{
  "name": "@starter/ts",
  "version": "0.4.3",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/zoeyzhao19/pkg-name#readme",
  "bugs": "https://github.com/zoeyzhao19/pkg-name/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeyzhao19/pkg-name.git"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md",
    "dev": "unbuild --stub",
    "lint": "eslint --fix ./src/**",
    "prepare": "husky install",
    "release": "bump --commit \"release: v%s\" --tag --push --all",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "version": "pnpm changelog"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.40.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "commitlint": "^17.6.7",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.34.1"
  },
  "packageManager": "pnpm@7.33.6"
}