{
  "name": "@starter/ts",
  "version": "0.5.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/zoeyzhao19/pkg-name#readme",
  "bugs": "https://github.com/zoeyzhao19/pkg-name/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeyzhao19/pkg-name.git"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --fix ./src/**",
    "prepare": "husky install",
    "release": "vitest run && changelogen --release && git push --follow-tags && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.43.1",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "changelogen": "^0.5.5",
    "commitlint": "^17.7.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@7.33.6"
}