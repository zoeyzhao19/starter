{
  "name": "@starter/ts",
  "version": "1.3.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/zoeyzhao19/pkg-name#readme",
  "bugs": "https://github.com/zoeyzhao19/pkg-name/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeyzhao19/pkg-name.git"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --fix ./src/**",
    "prepack": "nr build",
    "prepare": "husky",
    "release": "vitest run && changelogen --release && git push --follow-tags && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "changelogen": "^0.5.7",
    "commitlint": "^19.7.1",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.6"
  }
}